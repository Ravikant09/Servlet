<web-app>
  <display-name>ServletLab5</display-name>
  
  
  <context-param>
  	<param-name>website</param-name>
  	<param-value>www.gmail.com</param-value>
  </context-param>
  
  
  
  
  <servlet>
  <servlet-name>helloServ</servlet-name>
  <servlet-class>com.sunny.servlets.HelloServlet</servlet-class>
  
  <init-param>
  
  <param-name>email</param-name>
  <param-value>Sunny@gmail.com</param-value>
  
  </init-param>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>helloServ</servlet-name>
  <url-pattern>/hello.jlc</url-pattern>
  
  </servlet-mapping>
  


  <servlet>
  <servlet-name>haiServ</servlet-name>
  <servlet-class>com.sunny.servlets.HaiServlet</servlet-class>
  
  <init-param>
  
  <param-name>email</param-name>
  <param-value>Mohan@gmail.com</param-value>
  
  </init-param>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>haiServ</servlet-name>
  <url-pattern>/hai.jlc</url-pattern>
  
  </servlet-mapping>



</web-app>