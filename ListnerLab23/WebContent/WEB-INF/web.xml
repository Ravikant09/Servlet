
<web-app >
  <display-name>ListenerLab23</display-name>
  <welcome-file-list>
 
   <welcome-file>index.jsp</welcome-file>

  </welcome-file-list>

	<servlet>
	<servlet-name>MyServlet</servlet-name>
	<servlet-class>com.sunny.Listeners.LogoutServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
	<servlet-name>MyServlet</servlet-name>
	<url-pattern>/logout.sunny</url-pattern>
	</servlet-mapping>

	<listener>
	<listener-class>com.sunny.Listeners.MyContextListener</listener-class>
	</listener>


	<listener>
	<listener-class>com.sunny.Listeners.MySessionListener</listener-class>
	</listener>

</web-app>